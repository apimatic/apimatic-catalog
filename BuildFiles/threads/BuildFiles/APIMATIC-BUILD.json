{
  "$schema": "https://titan.apimatic.io/api/build/schema",
  "buildFileVersion": "1",
  "generatePortal": {
    "tailIncludes": "<script defer src='https://threads-apimatic-catalog.pages.dev/static/scripts/postYourFirstThread.js'></script><script>document.addEventListener('DOMContentLoaded', (event) => { APIMaticDevPortal.ready(({ registerWorkflow }) => {registerWorkflow('page:guided-walkthroughs/postYourFirstThread','Post your first thread', postYourFirstThread);});});</script>",

        "/updateCache": {
            "patch": {
                "description": "Update cache",
                "tags": [
                    "HLR"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "schema": {
                            "type": "object",
                            "required": [
                                "provider",
                                "is_cached"
                            ],
                            "properties": {
                                "provider": {
                                    "type": "string",
                                    "enum": [
                                        "Mitto",
                                        "Infobip",
                                        "MessageCloud",
                                        "HOT Mobile"
                                    ]
                                },
                                "ttl": {
                                    "type": "integer",
                                    "minimum": 600000
                                },
                                "is_cached": {
                                    "type": "boolean"
                                }
                            },
                            "additionalProperties": false
                        }
                    }
                ],
                "security": [
                    {
                        "APIKeyHeader": []
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Ok",
                        "schema": {
                            "description": "Ok",
                            "type": "object",
                            "properties": {
                                "isUpdated": {
                                    "type": "boolean"
                                },
                                "error": {
                                    "type": "boolean"
                                }
                            }
                        }
                    },
                    "501": {
                        "description": "Unauthorized",
                        "schema": {
                            "description": "Unauthorized",
                            "type": "object",
                            "properties": {
                                "isUpdated": {
                                    "type": "boolean"
                                },
                                "error": {
                                    "type": "boolean"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/reports/{job_id}": {
            "get": {
                "description": "Resend job report if it exists",
                "tags": [
                    "Utils"
                ],
                "parameters": [
                    {
                        "type": "string",
                        "format": "uuid",
                        "required": true,
                        "in": "path",
                        "name": "job_id"
                    },
                    {
                        "in": "header",
                        "name": "X-Api-Key",
                        "required": true,
                        "type": "string"
                    }
                ],
                "security": [
                    {
                        "APIKeyHeader": []
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "description": "OK",
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "boolean",
                                    "default": false
                                },
                                "report": {
                                    "type": "boolean",
                                    "default": true
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "description": "Bad Request",
                            "type": "object",
                            "properties": {
                                "statusCode": {
                                    "type": "number",
                                    "default": 400,
                                    "example": 400
                                },
                                "error": {
                                    "type": "string",
                                    "default": "Bad Request",
                                    "example": "Bad Request"
                                },
                                "message": {
                                    "type": "string",
                                    "default": "params.job_id should match format uuid",
                                    "example": "params.job_id should match format uuid"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "description": "Unauthorized",
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "boolean",
                                    "default": true
                                },
                                "report": {
                                    "type": "boolean",
                                    "default": false
                                }
                            }
                        }
                    }
                }
            }
        },
        "/estimate": {
            "post": {
                "description": "Estimate numbers",
                "tags": [
                    "Utils"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "numbers": {
                                    "type": "array",
                                    "items": {
                                        "type": "integer"
                                    }
                                },
                                "callback": {
                                    "type": "string",
                                    "pattern": "((?:(?:https?):(?:\\/\\/|\\\\\\\\)(?:www\\.)?|www\\.)[\\w\\d:#@%/;$()~_?\\\\+,\\-=\\\\.&]+)"
                                }
                            },
                            "required": [
                                "numbers"
                            ]
                        }
                    },
                    {
                        "in": "header",
                        "name": "X-Api-Key",
                        "required": true,
                        "type": "string"
                    }
                ],
                "security": [
                    {
                        "APIKeyHeader": []
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Ok",
                        "schema": {
                            "description": "Ok",
                            "type": "object",
                            "properties": {
                                "completed": {
                                    "type": "string"
                                },
                                "estimate": {
                                    "type": "number"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "description": "Unauthorized",
                            "type": "object",
                            "properties": {
                                "Unauthorized": {
                                    "type": "boolean"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/lookup": {
            "post": {
                "description": "Number lookup",
                "tags": [
                    "HLR"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "schema": {
                            "type": "object",
                            "required": [
                                "numbers",
                                "callback"
                            ],
                            "properties": {
                                "numbers": {
                                    "type": "array",
                                    "items": {
                                        "type": "integer"
                                    }
                                },
                                "callback": {
                                    "type": "string",
                                    "pattern": "((?:(?:https?):(?:\\/\\/|\\\\\\\\)(?:www\\.)?|www\\.)[\\w\\d:#@%/;$()~_?\\\\+,\\-=\\\\.&]+)"
                                }
                            }
                        }
                    }
                ],
                "security": [
                    {
                        "APIKeyHeader": []
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Ok",
                        "schema": {
                            "description": "Ok",
                            "type": "object",
                            "properties": {
                                "completed": {
                                    "type": "string"
                                },
                                "job_id": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "description": "Unauthorized",
                            "type": "object",
                            "properties": {
                                "Unauthorized": {
                                    "type": "boolean"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/v1/user": {
            "post": {
                "tags": [
                    "User"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "acc_id": {
                                    "type": "string"
                                },
                                "product_id": {
                                    "type": "string"
                                },
                                "rate_plan_id": {
                                    "type": "number"
                                },
                                "ip": {
                                    "type": "string"
                                },
                                "api_key": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "acc_id",
                                "rate_plan_id",
                                "product_id",
                                "ip",
                                "api_key"
                            ],
                            "additionalProperties": false
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created",
                        "schema": {
                            "description": "Created",
                            "type": "object",
                            "properties": {
                                "created": {
                                    "type": "boolean"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "description": "Unauthorized",
                            "type": "object",
                            "properties": {
                                "Unauthorized": {
                                    "type": "boolean"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Unprocessable Entity",
                        "schema": {
                            "description": "Unprocessable Entity",
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                }
            },
            "patch": {
                "tags": [
                    "User"
                ],
                "parameters": [
                    {
                        "name": "body",
                        "in": "body",
                        "schema": {
                            "type": "object",
                            "properties": {
                                "acc_id": {
                                    "type": "string"
                                },
                                "product_id": {
                                    "type": "string"
                                },
                                "rate_plan_id": {
                                    "type": "number"
                                },
                                "ip": {
                                    "type": "string"
                                },
                                "api_key": {
                                    "type": "string"
                                }
                            },
                            "additionalProperties": false
                        }
                    },
                    {
                        "in": "header",
                        "name": "X-Api-Key",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Ok",
                        "schema": {
                            "description": "Ok",
                            "type": "object",
                            "properties": {
                                "updated": {
                                    "type": "boolean"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "description": "Unauthorized",
                            "type": "object",
                            "properties": {
                                "Unauthorized": {
                                    "type": "boolean"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "description": "Not Found",
                            "type": "object",
                            "properties": {
                                "Message": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Unprocessable Entity",
                        "schema": {
                            "description": "Unprocessable Entity",
                            "type": "object",
                            "properties": {
                                "error": {
                                    "type": "boolean"
                                }
                            }
                        }
                    }
                }
            }
        },
